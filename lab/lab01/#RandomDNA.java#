import java.util.*;

public class RandomDNA.java {
    public static void main(String[] args) {
	String s = "";
	Random gen = new Random();
	int n = 0;
	for (int i = 0; i < 20; i++) {
	    n = gen.nextInt(4);
	    if (n == 3)
		s += "A";
	    else if (n == 2)
		s += "T";
	    else if (n == 1)
		s += "C";
	    else
		s += "G";
	}
	// Prints out the final string
	System.out.println(s);
    }
}

 